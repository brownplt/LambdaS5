    }
  })

  if (LeftFirst) {
    let (px = %ToPrimitiveHint(l, "number"))
    let (py = %ToPrimitiveHint(r, "number"))
    rest(px, py)
  } else {
    let (py = %ToPrimitiveHint(r, "number"))
    let (px = %ToPrimitiveHint(l, "number"))
    rest(px, py)
  }
}

