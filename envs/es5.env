          if (nx === ny) {
            break ret false
          } else { null };

          // TODO(matt): not sure how to compare -0 vs. 0

          if (nx === +inf) {
            break ret false
          } else { null };

          if (ny === +inf) {
            break ret true
          } else { null };

          if (ny === -inf) {
            break ret false
          } else { null };

          if (nx === -inf) {
            break ret true
          } else { null };

          break ret prim("<", nx, ny)
        }
      } else {
        //break ret prim("strcmp", px, py)
        break ret prim("string<", px, py)
